<?xml version="1.0" encoding="utf-8" ?>

<Defs>
	<Vehicles.VehicleBuildDef ParentName="RT_BaseVehicleBuild"><!-- 建築型態的載具，基本上與BuildingDef相同-->
		<defName>RT_ObelBlitzA_Truck_Blueprint</defName>
		<label>Obel Blitz Ambulance</label>
		<description>a 4×4 truck for first aid.</description>
		<graphicData>
			<texPath>ObelBlitzA/vehicle</texPath>
			<drawSize>(2.7,5.4)</drawSize>
		</graphicData>
		<size>(2,4)</size>
		<statBases> 		
			<MaxHitPoints>500</MaxHitPoints>
			<WorkToBuild>7500</WorkToBuild>
			<Flammability>0.05</Flammability>
		</statBases>		
		<constructionSkillPrerequisite>6</constructionSkillPrerequisite>	
		<costList>
			<EngineMedium>1</EngineMedium>
			<ComponentFuelTank>2</ComponentFuelTank>
			<ComponentTransmission>1</ComponentTransmission>
			<ComponentWheelTire>4</ComponentWheelTire>
			<ComponentIndustrial>25</ComponentIndustrial>
			<Glass>80</Glass>
			<Plasteel>120</Plasteel>
			<Plastic>120</Plastic>
			<AluminiumBar>100</AluminiumBar>
			<Mechanism>20</Mechanism>
			<ElectronicComponents>5</ElectronicComponents>
		</costList>	
		<thingToSpawn>RT_ObelBlitzA_Truck</thingToSpawn>	<!-- 建造後生成的載具Kind,這是與建築唯一不一樣的-->
	</Vehicles.VehicleBuildDef>

	<Vehicles.VehicleDef ParentName="RT_BaseVehicle">
		<defName>RT_ObelBlitzA_Truck</defName>
		<label>Obel Blitz truck</label>
		<description>a 4×4 truck for first aid.</description>
		<buildDef>RT_ObelBlitzA_Truck_Blueprint</buildDef>		
		<graphicData>
			<texPath>ObelBlitzA/vehicle</texPath>
			<drawSize>(2.7,5.4)</drawSize>
		</graphicData>		
		<size>(2, 4)</size>
		<fillPercent>1</fillPercent>	
		<vehicleCategory>Transport</vehicleCategory>
		<statBases>
			<ArmorRating_Blunt>0.22</ArmorRating_Blunt>
			<ArmorRating_Sharp>0.38</ArmorRating_Sharp>
			<Flammability>0</Flammability>
			<MarketValue>2050</MarketValue>
		</statBases>		
		<vehicleStats>
			<Mass>1200</Mass>
			<MoveSpeed>11</MoveSpeed>
			<CargoCapacity>400</CargoCapacity>
			<RepairRate>0.5</RepairRate>
		</vehicleStats>
		<soundOneShotsOnEvent Inherit = "False">
			<li>
				<key>PawnEntered</key>
				<value>VVE_VehicleEnter</value>
			</li>
			<li>
				<key>PawnExited</key>
				<value>VVE_VehicleEnter</value>
			</li>
			<li>
				<key>IgnitionOn</key>
				<value>RT_WheeledVehicleStart</value>
			</li>
			<li>
				<key>IgnitionOff</key>
				<value>RT_WheeledVehicleStop</value>
			</li>
		</soundOneShotsOnEvent>
		<soundSustainersOnEvent Inherit = "False"> 
			<li>
				<key>(IgnitionOn, IgnitionOff)</key>
				<value>RT_WheeledVehicleEngine</value>
			</li>
		</soundSustainersOnEvent>
		<race>
			<baseBodySize>3.5</baseBodySize>
		</race>	
		<properties>
			<customRoadCosts AssignDefaults = "0.25"/>
			<worldSpeedMultiplier>1</worldSpeedMultiplier>
			<offRoadMultiplier>2</offRoadMultiplier>
			<winterSpeedMultiplier>4</winterSpeedMultiplier>
			<vehicleJobLimitations>
				<li>(RepairVehicle, 3)</li>
			</vehicleJobLimitations>		
			<roles>
				<li>
					<key>driver</key>
					<label>Driver</label>
					<slots>2</slots>
					<slotsToOperate>1</slotsToOperate>					
					<handlingTypes>
						<li>Movement</li>
					</handlingTypes>
					<hitbox>
						<cells>
							<li>(0, 1)</li>
							<li>(1, 1)</li>
						</cells>
					</hitbox>	
				</li>
				<li>
					<key>passenger</key>
					<label>Passenger</label>
					<slots>4</slots>
					<slotsToOperate>1</slotsToOperate>					
					<handlingTypes>Turret</handlingTypes>
					<turretIds>
						<li>mainTurret</li>
					</turretIds>
					<hitbox>
						<cells>
							<li>(0, 0)</li>
							<li>(1, 0)</li>
							<li>(0, -1)</li>
							<li>(1, -1)</li>
						</cells>
					</hitbox>	
				</li>
			</roles>
		</properties>	
		<components>
			<!-- Internal mobile Part 內部驅動部件-->
			<li><!-- Engine 100-->
				<key>Engine</key>
				<label>Engine</label>
				<health>100</health>
				<priorityStatEfficiency>true</priorityStatEfficiency>
				<categories><li>MoveSpeed</li></categories>	
				<depth>Internal</depth>		
				<reactors>
					<li Class = "Vehicles.Reactor_Explosive">
						<indicator>VF_Explosive</indicator>
						<chance>0.25</chance>
						<maxHealth>0.3</maxHealth>
						<radius>3</radius>
						<damageDef>Bomb</damageDef>
					</li>
				</reactors>
				<hitbox>
					<cells>
						<li>(0, 2)</li>
						<li>(1, 2)</li>
					</cells>
				</hitbox>						
			</li>
			<li><!-- LeftFrontSuspension 40-->
				<key>LeftFrontSuspension</key>
				<label>left front Suspension</label>
				<health>40</health>
				<depth>Internal</depth>			
				<hitbox>
					<cells>
						<li>(0, 2)</li>
					</cells>
				</hitbox>
				<categories><li>MoveSpeed</li></categories>
			</li>
			<li><!-- RightFrontSuspension 40-->
				<key>RightFrontSuspension</key>
				<label>right front suspension</label>
				<health>40</health>
				<depth>Internal</depth>
				<hitbox>
					<cells>
						<li>(1, 2)</li>
					</cells>
				</hitbox>
				<categories><li>MoveSpeed</li></categories>
			</li>
			<li><!-- LeftRearSuspension 40-->
				<key>LeftRearSuspension</key>
				<label>left rear suspension</label>
				<health>40</health>
				<depth>Internal</depth>
				<hitbox>
					<cells>
						<li>(0, -1)</li>
					</cells>
				</hitbox>
				<categories><li>MoveSpeed</li></categories>
			</li>
			<li><!-- RightRearSuspension 40-->
				<key>RightRearSuspension</key>
				<label>right rear suspension</label>
				<health>40</health>
				<depth>Internal</depth>
				<hitbox>
					<cells>
						<li>(1, -1)</li>
					</cells>
				</hitbox>
				<categories><li>MoveSpeed</li></categories>
			</li>
			<li><!-- Gearbox 45-->
				<key>Gearbox</key>
				<label>gear box</label>
				<health>45</health>
				<depth>Internal</depth>
				<hitbox>
					<cells>
						<li>(0, 1)</li>
						<li>(1, 1)</li>
					</cells>
				</hitbox>
				<categories><li>MoveSpeed</li></categories>
			</li>
			<li><!-- FuelTank 40-->
				<key>FuelTank</key>
				<label>fuel tank</label>
				<health>40</health>
				<depth>Internal</depth>			
				<reactors>
					<li Class = "Vehicles.Reactor_FuelLeak"/>
				</reactors>			
				<hitbox>
					<cells>
						<li>(0, 0)</li>
						<li>(1, 0)</li>
					</cells>
				</hitbox>
			</li>
		<!-- External mobile Part 外部驅動部件-->
			<li><!-- LeftFrontWheel 30-->
				<key>LeftFrontWheel</key>
				<label>left front Wheel</label>
				<health>30</health>
				<depth>External</depth>
				<categories><li>MoveSpeed</li></categories>
				<tags><li>Wheel</li></tags>	
				<armor>
					<ArmorRating_Blunt>0</ArmorRating_Blunt>
					<ArmorRating_Sharp>0</ArmorRating_Sharp>
				</armor>			
				<hitbox>
					<fallthrough>false</fallthrough>
					<cells>
						<li>(0, 2)</li>
					</cells>
				</hitbox>			
			</li>
			<li><!-- RightFrontWheel 30-->
				<key>RightFrontWheel</key>
				<label>right front Wheel</label>
				<health>30</health>
				<depth>External</depth>
				<categories><li>MoveSpeed</li></categories>
				<tags><li>Wheel</li></tags>	
				<armor>
					<ArmorRating_Blunt>0</ArmorRating_Blunt>
					<ArmorRating_Sharp>0</ArmorRating_Sharp>
				</armor>
				<hitbox>
					<fallthrough>false</fallthrough>
					<cells>
						<li>(1, 2)</li>
					</cells>
				</hitbox>
			</li>
			<li><!-- LeftRearWheel	30-->
				<key>LeftRearWheel</key>
				<label>left rear wheel</label>
				<health>30</health>
				<depth>External</depth>
				<categories><li>MoveSpeed</li></categories>
				<tags><li>Wheel</li></tags>	
				<armor>
					<ArmorRating_Blunt>0</ArmorRating_Blunt>
					<ArmorRating_Sharp>0</ArmorRating_Sharp>
				</armor>
				<hitbox>
					<fallthrough>false</fallthrough>
					<cells>
						<li>(0, -1)</li>
					</cells>
				</hitbox>
			</li>
			<li><!-- RightRearWheel 30-->
				<key>RightRearWheel</key>
				<label>right rear Wheel</label>
				<health>30</health>
				<depth>External</depth>
				<categories><li>MoveSpeed</li></categories>
				<tags><li>Wheel</li></tags>
				<armor>
					<ArmorRating_Blunt>0</ArmorRating_Blunt>
					<ArmorRating_Sharp>0</ArmorRating_Sharp>
				</armor>
				<hitbox>
					<fallthrough>false</fallthrough>
					<cells>
						<li>(1, -1)</li>
					</cells>
				</hitbox>
			</li>
		<!-- External Shell Part 外殼部件-->
			<li><!-- EngineHood 40-->
				<key>EngineHood</key>
				<label>engine hood</label>
				<health>60</health>
				<hitbox><side>Body</side></hitbox>
				<depth>External</depth>
			</li>
			<li><!-- Roof 40-->
				<key>Roof</key>
				<label>roof</label>
				<health>60</health>
				<hitbox><side>Body</side></hitbox>
				<depth>External</depth>
			</li>
			<li><!-- BackupTire 30-->
				<key>CargoHold</key>
				<label>cargo hold</label>
				<health>60</health>
				<hitbox><side>Body</side></hitbox>
				<depth>External</depth>
			</li>
			<li><!-- LeftDoorPanel 40-->
				<key>LeftDoorPanel</key>
				<label>left door panel</label>
				<health>60</health>
				<depth>External</depth>			
				<hitbox><side>Left</side></hitbox>
			</li>
			<li><!-- RightDoorPanel 40-->
				<key>RightDoorPanel</key>
				<label>right door panel</label>
				<health>60</health>
				<depth>External</depth>
				<hitbox><side>Right</side></hitbox>
			</li>
			<li><!-- FrontBumper 30-->
				<key>FrontBumper</key>
				<label>front bumper</label>
				<health>45</health>
				<depth>External</depth>
				<hitbox><side>Front</side></hitbox>
			</li>
			<li><!-- RearBumper 25-->
				<key>RearBumper</key>
				<label>rear bumper</label>
				<health>38</health>
				<depth>External</depth>
				<hitbox><side>Back</side></hitbox>
			</li>
			
		<!-- Internal Shell Part 內裝部件-->
			<li><!-- Interior 30-->
				<key>Interior</key>
				<label>interior</label>
				<health>30</health>
				<depth>Internal</depth>
			</li>
			<li><!-- Dashboard 30-->
				<key>Dashboard</key>
				<label>dashboard</label>
				<health>30</health>
				<depth>Internal</depth>
			</li>
			<li><!-- SteeringWheel 20-->
				<key>SteeringWheel</key>
				<label>steering wheel</label>
				<health>20</health>
				<depth>Internal</depth>
			</li>
		</components>
		<comps>
			<li Class="Vehicles.CompProperties_FueledTravel">
				<fuelType>Chemfuel</fuelType>
				<leakDef>Filth_Fuel</leakDef>
				<fuelConsumptionRate>18</fuelConsumptionRate>
				<fuelConsumptionWorldMultiplier>0.5</fuelConsumptionWorldMultiplier>
				<fuelCapacity>120</fuelCapacity>
			</li>
		</comps>
	</Vehicles.VehicleDef>
</Defs>